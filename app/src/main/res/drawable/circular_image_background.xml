<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Contorno circular -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/color_botones" /> <!-- Color del contorno -->
        </shape>
    </item>

    <!-- Imagen recortada en forma circular -->
    <item
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp">
        <shape android:shape="oval">
            <solid android:color="#FFFFFF" /> <!-- Fondo blanco dentro del contorno -->
        </shape>
    </item>
</layer-list>